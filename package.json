{
  "name": "rlp-encoding",
  "version": "1.0.1",
  "description": "Abstract-encoding interface for RLP",
  "keywords": [
    "ethereum",
    "rlp"
  ],
  "homepage": "https://github.com/fanatid/rlp-encoding",
  "bugs": {
    "url": "https://github.com/fanatid/rlp-encoding/issues"
  },
  "license": "MIT",
  "author": "Kirill Fomichev <fanatid@ya.ru> (https://github.com/fanatid)",
  "files": [
    "es5",
    "lib"
  ],
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/fanatid/rlp-encoding"
  },
  "scripts": {
    "build": "BABEL_ENV=es5 babel lib -d es5",
    "coverage": "nyc --require babel-register tape test/*.js",
    "coverage-lcov": "npm run coverage && nyc report -r lcov",
    "lint": "standard",
    "prepublish": "npm run build",
    "test": "npm run lint && npm run unit",
    "unit": "tape -r babel-register test/*.js"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.14.0",
    "nyc": "^8.3.0",
    "standard": "*",
    "tape": "^4.6.0"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
